script.

    function calculateYears() {
        var oneDay = 24*60*60*1000; // hours*minutes*seconds*milliseconds
        var oneYear = 365.25;
        var startDate = new Date(2005, 08, 23);

        var diffDays = Math.abs(((new Date()).getTime() - startDate.getTime())/(oneDay));
        var years = diffDays / oneYear;

        var el = document.querySelector("#yearCount")
        el.textContent = years.toFixed(8);
    }

    (function() {
        calculateYears();
        setInterval(calculateYears, 1000);
    })();
